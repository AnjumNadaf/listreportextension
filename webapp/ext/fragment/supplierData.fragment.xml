                <Dialog title="My Dialog" id="openD2">
                                <content>
                                      <Table id="table4" mode="MultiSelect" growingThreshold="100" growing="true" 
     growingScrollToLoad="true"  items="{data>/dataSet2}" width="50rem">
        <columns>
         <Column>
					<Label text="Supplierid"></Label>
				</Column>
				
				<Column>
					<Label text="Suppliername"></Label>
				</Column>
				<Column>
					<Label text="Productid"></Label>
				</Column>
				<Column>
					<Label text="Quantity"></Label>
				</Column>
				
				<Column>
					<Label text="Description"></Label>
				</Column>
        </columns>
        <items>
        <ColumnListItem>
					<cells>
						<Text text="{data>Supplierid}" visible="true"></Text>
						<Text text="{data>Suppliername}"></Text>
						<Text text="{data>Productid}"></Text>
						<Text text="{data>Quantity}"></Text>
						<Text text="{data>Description}"></Text>
                        	
						
